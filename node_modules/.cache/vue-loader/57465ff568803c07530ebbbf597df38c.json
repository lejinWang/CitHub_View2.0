ht\" class=\"breadcrumb\">\r\n        <el-breadcrumb-item :to=\"{ path: '/' }\" @click=\"goHome\">Service</el-breadcrumb-item>\r\n        <el-breadcrumb-item>{{ serviceInfor.title }}</el-breadcrumb-item>\r\n      </el-breadcrumb>\r\n      <el-col :xs=\"{ span: 12, offset: 0 }\" :sm=\"{ span: 12, offset: 0 }\" :md=\"{ span: 18, offset: 0 }\" :lg=\"{ span: 18, offset: 0 }\" :xl=\"{ span: 20, offset: 0 }\">\r\n        <el-col :span=\"24\"\r\n          ><h1 style=\"float: left\">{{ serviceInfor.title }}</h1>\r\n          <star style=\"float: right; font-size: 1rem; padding: 10px; text-algin: right\"\r\n            ><el-button style=\"font-size: 1rem; padding: 5px 10px\" @click=\"starClick\"><i :class=\"starData.starClass\"></i> Star</el-button><el-label style=\"border: 1px solid #dee2e6; padding: 2px 10px 4.5px 9px; border-radius: 4px\">{{ starData.starNum }} </el-label></star\r\n          ></el-col\r\n        >\r\n        <el-col :span=\"24\" style=\"margin-bottom: 10px\">\r\n          <el-tag v-for=\"item in serviceInfor.tags.split(',')\" :key=\"item\" effect=\"dark\" style=\"margin-right: 10px; height: 20px; font-size: 10px; padding: 0px 5px 2px; display: unset\">\r\n            {{ item }}\r\n          </el-tag>\r\n        </el-col>\r\n        <el-col :span=\"24\" style=\"margin-bottom: 10px\">\r\n          <label>\r\n            Author: <author>{{ serviceInfor.author }} </author>\r\n          </label>\r\n          <label>\r\n            Version: <version>{{ serviceInfor.version }} </version>\r\n          </label>\r\n          <label>\r\n            Last Updated: <updatetime>{{ serviceInfor.time }} </updatetime>\r\n          </label>\r\n          <label>\r\n            License: <license>{{ serviceInfor.license }}</license>\r\n          </label>\r\n        </el-col>\r\n        <el-col :span=\"24\" style=\"margin-bottom: 10px\">\r\n          Repository:\r\n          <repository\r\n            ><a :href=\"serviceInfor.repository\" style=\"font-weight: bold\">{{ serviceInfor.repository }}</a></repository\r\n          >\r\n        </el-col>\r\n        <el-col :span=\"24\" style=\"margin-bottom: 20px\">\r\n          {{ serviceInfor.short_desc }}\r\n        </el-col>\r\n        <el-col :span=\"24\" style=\"padding: 10px 20px; font-size: 1rem; margin-bottom: 20px; color: #0c5460; background-color: #d1ecf1; border-color: #bee5eb\">\r\n          Service URL (POST method):\r\n          <url>{{ serviceInfor.url }}</url>\r\n        </el-col>\r\n      </el-col>\r\n\r\n      <el-col :xs=\"{ span: 12, offset: 0 }\" :sm=\"{ span: 12, offset: 0 }\" :md=\"{ span: 6, offset: 0 }\" :lg=\"{ span: 6, offset: 0 }\" :xl=\"{ span: 4, offset: 0 }\" style=\"text-align: right\">\r\n        <div><img :src=\"serviceInfor.img\" style=\"height: auto\" /></div>\r\n        <el-rate v-model=\"serviceInfor.scores\" disabled show-score text-color=\"#ff9900\" score-template=\"{value}\"> </el-rate>\r\n      </el-col>\r\n      <el-col :span=\"24\" style=\"margin-bottom: 10px; z-index: 0\"\r\n        ><el-tabs v-model=\"tabsActiveName\" type=\"card\" @tab-click=\"handleClick\">\r\n          <el-tab-pane label=\"Details\" name=\"details\">\r\n            <p>{{ serviceInfor.description }}</p>\r\n            <h2 style=\"margin-top: 30px\">Inputs</h2>\r\n            <el-table :data=\"xput_format.input\" stripe style=\"width: 100%; font-size: 1rem\" :header-cell-style=\"{ background: '#343a40', color: '#fff' }\">\r\n              <el-table-column prop=\"parameter\" label=\"Parameter\" width=\"180\"> </el-table-column>\r\n              <el-table-column prop=\"type\" label=\"Type\" width=\"180\"> </el-table-column>\r\n              <el-table-column prop=\"desc\" label=\"Description\"> </el-table-column>\r\n              <el-table-column prop=\"example\" label=\"Example\"> </el-table-column>\r\n            </el-table>\r\n            <h2 style=\"margin-top: 30px\">outputs</h2>\r\n            <el-table :data=\"xput_format.output\" stripe style=\"width: 100%; font-size: 1rem\" :header-cell-style=\"{ background: '#343a40', color: '#fff' }\">\r\n              <el-table-column prop=\"parameter\" label=\"Parameter\" width=\"180\"> </el-table-column>\r\n              <el-table-column prop=\"type\" label=\"Type\" width=\"180\"> </el-table-column>\r\n              <el-table-column prop=\"desc\" label=\"Description\"> </el-table-column>\r\n              <el-table-column prop=\"example\" label=\"Example\"> </el-table-column>\r\n            </el-table>\r\n          </el-tab-pane>\r\n          <el-tab-pane label=\"Evaluation\" name=\"evaluation\">\r\n            <p>The following table gives the sizes of test suites, computational costs (seconds) and success rate.</p>\r\n            <el-table :data=\"evaluateData\" :span-method=\"objectSpanMethod\" stripe style=\"width: 100%; font-size: 1rem\" :header-cell-style=\"{ background: '#343a40', color: '#fff' }\">\r\n              <el-table-column prop=\"category\" label=\"Category\"> </el-table-column>\r\n              <el-table-column prop=\"strength\" label=\"Strength\"> </el-table-column>\r\n              <el-table-column prop=\"total_size\" label=\"Total Size\"> </el-table-column>\r\n              <el-table-column prop=\"total_time\" label=\"Total Time\"> </el-table-column>\r\n              <el-table-column prop=\"success_rate\" label=\"Success Rate\"> </el-table-column>\r\n              <el-table-column fixed=\"right\" label=\"\" width=\"120\">\r\n                <template #default=\"scope\">\r\n                  <el-button @click.prevent=\"handelDetail(serviceInfor.id, scope.row)\" type=\"text\"> detail </el-button>\r\n                </template>\r\n              </el-table-column>\r\n            </el-table>\r\n            <!-- 详细信息-->\r\n            <el-dialog :title=\"dialogData.dialog_title\" v-model=\"dialogData.detailDialog\">\r\n              <p style=\"margin: 0px\">This benchmark set consists of 59 test models. The detailed results can be found in the following table.</p>\r\n              <el-col :span=\"24\" style=\"margin: 0px\">\r\n                <el-tag v-for=\"item in dialogData.dialog_tags.split(',')\" :key=\"item\" effect=\"dark\" style=\"margin-right: 10px; height: 20px; font-size: 10px; padding: 0px 5px 2px; display: unset\">\r\n                  {{ item }}\r\n                </el-tag>\r\n              </el-col>\r\n              <el-table :data=\"evaluateDetailData.values\" :header-cell-style=\"{ background: '#343a40', color: '#fff' }\">\r\n                <el-table-column property=\"name\" label=\"Name\"></el-table-column>\r\n                <el-table-column property=\"size\" label=\"Size\"></el-table-column>\r\n                <el-table-column property=\"time\" label=\"Time\"></el-table-column>\r\n                <el-table-column property=\"success_rate\" label=\"Success Rate\"></el-table-column>\r\n              </el-table>\r\n            </el-dialog>\r\n          </el-tab-pane>\r\n          <el-tab-pane label=\"History\" name=\"history\">\r\n            <div class=\"block\">\r\n              <el-timeline>\r\n                <el-timeline-item v-for=\"item in historyData\" :key=\"item.id\" :timestamp=\"item.date\" placement=\"top\">\r\n                  <el-card>\r\n                    <h4>Version{{ item.version }}</h4>\r\n                    <p>{{ item.description }}</p>\r\n                  </el-card>\r\n                </el-timeline-item>\r\n              </el-timeline>\r\n            </div>\r\n          </el-tab-pane>\r\n          <el-tab-pane label=\"Ratings & Reviews\" name=\"rate_reviews\">\r\n            <el-row>\r\n              <el-col :span=\"7\"\r\n                ><font style=\"font-weight: bold; font-size: 1.5rem\">Ratings </font>\r\n                <el-tag effect=\"dark\" style=\"font-weight: bold; margin-right: 10px; height: 20px; font-size: 1.5rem; padding: 0px 5px 2px; display: unset; border-radius: 20%\">\r\n                  {{ serviceInfor.scores }}\r\n                </el-tag>\r\n                <span style=\"margin-top: 10px; display: block\">{{ rateData.peopleNum }} ratings (out of 5)</span>\r\n              </el-col>\r\n              <el-col :span=\"17\" style=\"padding: 0px; text-align: left; min-height: 60px\">\r\n                <el-rate id=\"rateCol\" :disabled=\"rateData.disabled\" v-model=\"rateData.value\" :colors=\"rateData.colors\" @change=\"handleRate()\"> </el-rate>\r\n                <p v-if=\"rateData.disabled\" style=\"margin-top: 22px\">{{ rateData.value }} points Your had scored !</p>\r\n                <p style=\"height: 20px\"></p>\r\n              </el-col>\r\n            </el-row>\r\n            <el-row>\r\n              <el-col :span=\"4\">\r\n                <font style=\"font-weight: bold; font-size: 1.5rem; margin-top: 20px\">Reviews </font>\r\n                <span style=\"margin-top: 10px; display: block\">{{ reviewData.peopleNum }} reviews</span>\r\n              </el-col>\r\n\r\n              <el-col :span=\"20\" style=\"padding: 0px; text-align: left\">\r\n                <el-row style=\"margin-bottom: 10px\">\r\n                  <el-col :span=\"2\">\r\n                    <el-avatar :size=\"55\" style=\"font-size: 1.5rem; padding: 2px\">{{ user.name.substr(0, 1).toUpperCase() }}</el-avatar>\r\n                  </el-col>\r\n                  <el-col :span=\"22\">\r\n                    <el-input style=\"width: 98%; margin-left: 2%\" type=\"textarea\" placeholder=\"comments\" v-model=\"reviewData.content\" maxlength=\"30\" @focus=\"reviewData.showCommentSubmit = true\"> </el-input>\r\n                    <el-button v-show=\"reviewData.showCommentSubmit\" style=\"margin: 15px 0px 0px 2%; float: left\" type=\"primary\" @click=\"addNewComment()\">Submit</el-button>\r\n                  </el-col>\r\n                </el-row>\r\n                <!--评论-->\r\n                <div v-for=\"(item,index) in reviewData.commentsData\" :key=\"item.id\">\r\n                  <el-col :span=\"2\">\r\n                    <el-avatar :size=\"55\" style=\"font-size: 1.5rem\">{{ item.post_name.substr(0, 1).toUpperCase() }}</el-avatar>\r\n                  </el-col>\r\n                  <el-col :span=\"22\">\r\n                    <el-col :span=\"24\">\r\n                      <el-label style=\"margin-left: 2%; float: left; font-size: 1.5rem; font-weight: 500; line-height: 1\"\r\n                        >{{ item.post_name }}<br /><span style=\"font-size: 1rem; font-weight: 200\">{{ item.date }}</span></el-label\r\n                      >\r\n                      <el-label style=\"float: right; margin-right: 2%\">\r\n                        <el-button style=\"font-size: 20px\" icon=\"el-icon-chat-dot-square\" @click=\"()=>{reviewData.root_id=item.id;reviewData.post_id=user.id;reviewData.post_name=user.name;reviewData.to_id=item.post_id;reviewData.to_name=item.post_name;item.showReply=!item.showReply}\" circle></el-button> <font style=\"font-size: 1.27rem\"> {{ item.reply.length }}</font></el-label\r\n                      >\r\n                     \r\n                    </el-col>\r\n                    <el-col :span=\"24\">\r\n                      <el-label style=\"float: left; margin: 2%\">{{ item.content }}</el-label>\r\n                    </el-col>\r\n                    \r\n                    <!--回复-->\r\n                    <div v-for=\"reply_item in item.reply\" :key=\"reply_item.id\">\r\n                      <el-col :span=\"2\">\r\n                        <el-avatar :size=\"50\" style=\"font-size: 1.5rem\">{{ reply_item.post_name.substr(0, 1).toUpperCase() }}</el-avatar>\r\n                      </el-col>\r\n                      <el-col :span=\"22\">\r\n                        <el-col :span=\"24\">\r\n                          <el-label style=\"margin-left: 2%; float: left; font-size: 1.5rem; font-weight: 500; line-height: 1\"\r\n                            >{{ reply_item.post_name }}<br /><span style=\"font-size: 1rem; font-weight: 200\">{{ reply_item.date }}</span></el-label\r\n                          >\r\n                          <el-label style=\"float: right; margin-right: 2%\"> <el-button style=\"font-size: 20px\" icon=\"el-icon-chat-dot-square\" @click=\"()=>{reviewData.root_id=item.id;reviewData.post_id=user.id;reviewData.post_name=user.name;reviewData.to_id=reply_item.post_id;reviewData.to_name=reply_item.post_name;item.showReply=!item.showReply}\" circle></el-button></el-label>\r\n                        </el-col>\r\n                        <el-col :span=\"24\">\r\n                          <el-label style=\"float: left; margin: 2%\"\r\n                            ><span style=\"color: blue\">@{{ reply_item.to_name }}: </span>{{ reply_item.content }}</el-label\r\n                          >\r\n                        </el-col>\r\n                      </el-col>\r\n                    </div>\r\n                    <el-col :span=\"24\" v-show=\"item.showReply\" style=\"margin-bottom:10px\">\r\n                        <el-input style=\"width: 98%; margin-left: 2%\" type=\"textarea\" :placeholder=\"comments\" v-model=\"reviewData.reply_content\" maxlength=\"30\"> </el-input>\r\n                        <el-button style=\"margin: 15px 0px 0px 2%; float: left\" type=\"primary\" @click=\"addNewReply(index)\">Submit</el-button>\r\n                      </el-col>\r\n                  </el-col>\r\n                  <el-col :span=\"24\"> <el-divider></el-divider></el-col>\r\n                </div>\r\n              </el-col>\r\n            </el-row>\r\n          </el-tab-pane>\r\n          <el-tab-pane label=\"Demo\" name=\"demo\">         <vue-json-editor\r\n 3         v-model=\"reviewData\"  \r\n 4         :showBtns=\"false\"    \r\n 5         :mode=\"'code'\"  \r\n 6         lang=\"zh\" \r\n 7         @json-change=\"onJsonChange\"  \r\n 8         @json-save=\"onJsonSave\"  \r\n 9         /></el-tab-pane>\r\n        </el-tabs>\r\n      </el-col>\r\n    </el-col>\r\n  </el-row>\r\n</template>\r\n\r\n<script>\r\n// import ElementPlus from 'element-plus'\r\nimport vueJsonEditor from 