ents/service/customisation.vue","sourceRoot":"","sourcesContent":["<template>\r\n  <el-col :xs=\"{ span: 24, offset: 0 }\" :sm=\"{ span: 24, offset: 0 }\" :md=\"{ span: 6, offset: 0 }\" :lg=\"{ span: 6, offset: 0 }\" :xl=\"{ span: 6, offset: 0 }\"> <serviceTypeList></serviceTypeList> </el-col>\r\n  <el-col :xs=\"{ span: 24, offset: 0 }\" :sm=\"{ span: 24, offset: 0 }\" :md=\"{ span: 17, offset: 1 }\" :lg=\"{ span: 17, offset: 1 }\" :xl=\"{ span: 17, offset: 1 }\">\r\n    <div style=\"margin-bottom: 30px; width: 100%\">\r\n      <h1 style=\"font-size: 1.75rem; margin-top: 10px\">Service Customisation</h1>\r\n      <p class=\"text-secondary\" style=\"\">Service composition enables an agile and dynamic way to develop and grow software. Different services in CitHub can be combined together to accomplish particular testing processes.</p>\r\n      <el-row>\r\n        <el-steps :active=\"active\" finish-status=\"success\" simple style=\"margin-top: 20px\">\r\n          <el-step title=\"Step 1\"></el-step>\r\n          <el-step title=\"Step 2\"></el-step>\r\n          <el-step title=\"Step 3\"></el-step>\r\n        </el-steps>\r\n        <el-row>\r\n          <el-col :span=\"24\" v-show=\"active == 0\">\r\n            <h2 style=\"margin-top: 30px\">Step 1: Determine Workflow</h2>\r\n            <p class=\"simpleText\">Please select from the following two common testing workflows ...</p>\r\n            <el-col :span=\"11\" v-for=\"(item, index) in customisationData.stepOne\" :key=\"index\" :offset=\"index > 0 ? 1 : 0\">\r\n              <div style=\"padding: 10px; background-color: #f8f9fa\">\r\n                <h2 style=\"margin: 0px 10px\">{{ item.type }}</h2>\r\n                <div style=\"padding: 10px\">\r\n                  <p class=\"text-secondary\" style=\"height: 162px\">{{ item.desc }}</p>\r\n                  <div>\r\n                    <el-button  v-show=\"!item.selected\" type=\"primary\"   style=\"width: 100%; font-size: 1rem\" @click=\"chooseType(index)\" plain>select</el-button>\r\n                    <el-button   v-show=\"item.selected\" type=\"primary\"   style=\"width: 100%;    margin-left: 0px; font-size: 1rem\" @click=\"chooseType(index)\" >select</el-button>\r\n                  </div>\r\n                </div>\r\n              </div>\r\n            </el-col>\r\n          </el-col>\r\n          <el-col :span=\"24\" v-show=\"active == 1&&customisationData.stepOne[0].selected\">\r\n            <h2 style=\"margin-top: 30px\">Step 2: Identify Services</h2>\r\n            <p class=\"simpleText\">Please identify the services that will be assembled ...</p>\r\n            <el-row>\r\n              <el-col :span=\"24\"><div  class=\"grid-content\"><b>Sequence Workflow</b></div></el-col>\r\n              <el-col :span=\"24\">\r\n              <el-button type=\"primary\" class=\"plusBtn\" icon=\"el-icon-plus\"></el-button>\r\n              </el-col>\r\n            </el-row>\r\n          </el-col>\r\n\r\n           <el-col :span=\"24\" v-show=\"active == 1&&customisationData.stepOne[1].selected\">\r\n            <h2 style=\"margin-top: 30px\">Step 2: Identify Services</h2>\r\n            <p class=\"simpleText\">Please identify the services that will be assembled ...</p>\r\n            <el-row>\r\n              <el-col :span=\"24\"><div  class=\"grid-content\"><b>Parallel Workflow</b></div></el-col>\r\n            </el-row>\r\n          </el-col>\r\n\r\n          <el-col :span=\"24\" v-show=\"active == 2\">\r\n            <h2 style=\"margin-top: 30px\">Step 3: Service Execution</h2>\r\n            <p class=\"simpleText\">Simply type in the inputs and click the submit button to obtain the results ...</p>\r\n          </el-col>\r\n        </el-row>\r\n        <el-button :disabled=\"active == 2\" style=\"float: right; margin-top: 12px\" @click=\"next\">next</el-button>\r\n        <el-button :disabled=\"active == 0\" style=\"float: left; margin-top: 12px\" @click=\"previous\">previous</el-button>\r\n      </el-row>\r\n    </div>\r\n  </el-col>\r\n</template>\r\n\r\n<script>\r\nimport { reactive, ref } from \"vue\";\r\nimport serviceTypeList from \"./typelist.vue\";\r\nexport default {\r\n  setup() {\r\n    const customisationData = reactive({\r\n      stepOne: [\r\n        {\r\n          type: \"Sequence\",\r\n          desc: \"A sequence workflow allows you to assemble a set of services and execute them exactly one after another (for example, it can be used to first generate a test suite, and then reorder its execution sequence to maximise the rate of combination coverage).\",\r\n          selected: true,\r\n        },\r\n        {\r\n          type: \"Parallel\",\r\n          desc: \"A Parallel workflow allows you to assemble a set of services and execute them at the same time (for example, it can be used to evaluate the performance of several test suite generation services under the same subject test model).\",\r\n          selected: false,\r\n        },\r\n      ],\r\n    });\r\n    //setp1\r\n    const chooseType = (index) => {\r\n      customisationData.stepOne[index].selected = true;\r\n      let arrtemp = customisationData.stepOne.filter((data, idx) => idx !== index);\r\n      for (let index in arrtemp) {\r\n        arrtemp[index].selected = false;\r\n      }\r\n    };\r\n    const active = ref(0);\r\n    //\r\n    return { chooseType, customisationData, active };\r\n  },\r\n  methods: {\r\n    next() {\r\n      if (this.active++ > 1) this.active = 2;\r\n    },\r\n    previous() {\r\n      if (this.active-- < 1) this.active = 0;\r\n    },\r\n  },\r\n  components: { serviceTypeList },\r\n  directives: {\r\n  focus: {\r\n    // 指令的定义\r\n    inserted: function (el) {\r\n      el.focus()\r\n    }\r\n  }\r\n}\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.text-secondary {\r\n  color: #6c757d !important;\r\n}\r\n.simpleText {\r\n  color: #6c757d !important;\r\n  padding: 0px;\r\n  margin: 0px;\r\n  margin-bottom: 10px;\r\n}\r\nh2 {\r\n  font-size: 1.25rem;\r\n  font-weight: bold;\r\n  margin-bottom: 15px;\r\n  \r\n}\r\n.grid-content {\r\n   padding: .75rem 1.25rem;\r\n    margin-bottom: 0;\r\n    background-color: rgba(0, 0, 0, .03);\r\n    border-bottom: 1px solid rgba(0, 0, 0, .125);\r\n        margin-left: 10px;\r\n    color: #0D5F6F;\r\n        border-radius: calc(.25rem - 1px) calc(.25rem - 1px) 0 0;\r\n  }\r\n  .plusBtn{\r\n          padding: .75rem .75rem;\r\n          display: inline-block;\r\n\r\n  }\r\n  .el-icon-plus{\r\n       font-weight: 900 !important;\r\n  }\r\n</style>\r\n"]}]}