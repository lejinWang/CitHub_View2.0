ListData = Array(20).fill(data);\r\n      // localStorage.setItem(\"serviceList\",JSON.stringify(service_data))\r\n      // alert(JSON.stringify(service_data))\r\n      // RefChilde.value.reloadData();      //sonFn是子组件里的方法\r\n    }\r\n\r\n    return { RefChilde, keyword, searchByKeyword, serviceListData };\r\n  },\r\n  components: {\r\n    serviceTypeList,\r\n    serviceList,\r\n  },\r\n};\r\nfunction getNowDate() {\r\n  let date = new Date();\r\n  let y = date.getFullYear();\r\n  let m = date.getMonth() + 1;\r\n  let d = date.getDate();\r\n  // let H = date.getHours();\r\n  // let mm = date.getMinutes();\r\n  // let s=date.getSeconds()\r\n  m = m < 10 ? \"0\" + m : m;\r\n  d = d < 10 ? \"0\" + d : d;\r\n  // H = H < 10 ? \"0\" + H : H;\r\n  return y + \"-\" + m + \"-\" + d;\r\n}\r\n</script>\r\n\r\n<!-- Add \"scoped\" attribute to limit CSS to this component only -->\r\n<style scoped>\r\n.float-right {\r\n  float: right;\r\n}\r\nbody {\r\n  margin: 0px;\r\n}\r\na {\r\n  text-decoration: none;\r\n}\r\n\r\n.router-link-active {\r\n  text-decoration: none;\r\n}\r\n</style>\r\n"]}]}